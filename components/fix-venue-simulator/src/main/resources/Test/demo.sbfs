<?xml version="1.0" encoding="UTF-8"?>
<sbfs:Simulation xmi:version="2.0" xmlns:xmi="http://www.omg.org/XMI" xmlns:sbfs="http://www.streambase.com/schemas/feedsim/02/SBFS" Description="create new orders of various TIF types that include DayOrder and IOC">
  <SimStreams Name="ManuallyCreateNewOrder">
    <ProcessingOptions DataRatePerSecond="1.0" DataGenerationMethod="Custom"/>
    <Schema>
      <Fields Name="TimeInForce" DataTypeName="string" Description="">
        <Distribution GenerationMethod="Enumerated" UseRandomSize="true" NumRandomEnumValues="1" EnumeratedDistMethod="SpecifiedValues" BlobMinSize="4">
          <EnumeratedDist weight="0.4" value="0"/>
          <EnumeratedDist weight="0.6" value="3"/>
        </Distribution>
        <DataSourceMapping/>
      </Fields>
      <Fields Name="Symbol" DataTypeName="string" Description="">
        <Distribution GenerationMethod="Enumerated" UseRandomSize="true" NumRandomEnumValues="1" EnumeratedDistMethod="SpecifiedValues" BlobMinSize="4">
          <EnumeratedDist weight="0.1" value="SSO"/>
          <EnumeratedDist weight="0.1" value="OIL"/>
          <EnumeratedDist weight="0.1" value="XPP"/>
          <EnumeratedDist weight="0.1" value="USD/EUR"/>
          <EnumeratedDist weight="0.1" value="T Jan 15 Call"/>
          <EnumeratedDist weight="0.1" value="USGG30YR"/>
        </Distribution>
        <DataSourceMapping/>
      </Fields>
      <Fields Name="session" DataTypeName="string" Description="">
        <Distribution GenerationMethod="Enumerated" UseRandomSize="true" NumRandomEnumValues="1" EnumeratedDistMethod="SpecifiedValues" BlobMinSize="4">
          <EnumeratedDist weight="0.3" value="VENUE1"/>
          <EnumeratedDist weight="0.7" value="VENUE2"/>
        </Distribution>
        <DataSourceMapping/>
      </Fields>
    </Schema>
  </SimStreams>
</sbfs:Simulation>
